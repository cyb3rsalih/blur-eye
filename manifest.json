{
    "name": "Blur Image Extension",
    "version": "1.0",
    "description": "Blurs all images on webpages",
    "manifest_version": 2,
    "icons": {
        "16": "favicon.ico",
        "48": "favicon.ico",
        "128": "favicon.ico"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
    }],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "favicon.ico",
            "48": "favicon.ico",
            "128": "favicon.ico"
        }
    }
}